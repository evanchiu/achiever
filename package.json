{
  "name": "achiever",
  "version": "2.6.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && cp dist/index.html dist/404.html",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "0.27.2",
    "compare-versions": "4.1.3",
    "core-js": "3.23.3",
    "gw2e-chat-codes": "1.3.2",
    "vue": "^3.2.37",
    "vue-router": "4.1.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@vue/cli-plugin-babel": "5.0.7",
    "@vue/cli-plugin-eslint": "5.0.7",
    "@vue/cli-plugin-router": "5.0.7",
    "@vue/cli-service": "5.0.7",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/eslint-config-prettier": "7.0.0",
    "autoprefixer": "^10.4.7",
    "eslint": "8.19.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-vue": "^9.1.1",
    "lint-staged": "13.0.3",
    "postcss": "^8.4.14",
    "prettier": "2.7.1",
    "tailwindcss": "^3.1.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint"
    ],
    "*.{html,md}": [
      "prettier --write"
    ]
  }
}
